{
    "name": "webpack-config",
    "version": "1.0.0",
    "description": "",
    "private": true,
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "type-check": "tsc --noEmit",
        "type-check:watch": "tsc --noEmit --watch",
        "start": "concurrently --raw -k -n \"WEBPACK,TS,API\" -c \"cyan,magenta,green\" \"webpack serve --config webpack.dev.js\" \"npm run type-check:watch\" \"npm run api\"",
        "api": "json-server --watch src/app/data-model/db.json --port 4000",
        "build": "npm run type-check && webpack --config webpack.prod.js",
        "build-dev": "npm run type-check && webpack --config webpack.dev.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Yeison0416/webpack-config.git"
    },
