    },
    "homepage": "https://github.com/Yeison0416/webpack-config#readme",
    "devDependencies": {
        "@babel/core": "^7.28.4",
        "@babel/eslint-parser": "^7.28.4",
        "@babel/preset-env": "^7.28.3",
        "@babel/preset-typescript": "^7.27.1",
        "@eslint/js": "^9.36.0",
        "babel-loader": "^10.0.0",
        "clean-webpack-plugin": "^4.0.0",
        "concurrently": "^9.2.1",
